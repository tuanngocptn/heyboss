# Production Environment Variables for Vercel Deployment
# Copy these to your Vercel project environment variables

# Telegram Configuration
TELEGRAM_BOT_TOKEN=your_production_bot_token_here
TELEGRAM_CHAT_ID=-1003147773870
TELEGRAM_TOPIC_ID=3

# App Configuration
NODE_ENV=production
NEXT_TELEMETRY_DISABLED=1
NEXT_PUBLIC_APP_URL=https://heyboss.wtf
NEXT_PUBLIC_SITE_DOMAIN=heyboss.wtf

# File Configuration
REPORTS_DIR=./reports
MAX_PDF_SIZE=10485760
RATE_LIMIT_PER_HOUR=5

# Cloudflare Turnstile (Production Keys)
NEXT_PUBLIC_TURNSTILE_SITE_KEY=your_production_turnstile_site_key
TURNSTILE_SECRET_KEY=your_production_turnstile_secret_key

# Debug Settings
DEBUG_MODE=false
MOCK_TELEGRAM=false