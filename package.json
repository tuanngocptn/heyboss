{
  "name": "heyboss",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node --env-file=.env node_modules/.bin/next dev --turbopack --port 3000 --hostname 0.0.0.0",
    "dev:fast": "node --env-file=.env node_modules/.bin/next dev --turbopack --port 3000 --experimental-https",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "dev:watch": "node --env-file=.env node_modules/.bin/next dev --turbopack --port 3000 --experimental-app-dir",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "local:up": "npm run local:down && docker-compose up -d --wait && npm run db:deploy",
    "local:down": "docker-compose down -v --remove-orphans",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:all": "npm run type-check && npm run lint && npm test",
    "test:verbose": "jest --verbose",
    "test:api": "jest --testPathPatterns=api",
    "db:seed": "tsx prisma/seed/seed.ts"
  },
  "dependencies": {
    "@editorjs/code": "^2.9.3",
    "@editorjs/delimiter": "^1.4.2",
    "@editorjs/editorjs": "^2.31.0",
    "@editorjs/header": "^2.8.8",
    "@editorjs/list": "^2.0.8",
    "@editorjs/marker": "^1.4.0",
    "@editorjs/paragraph": "^2.11.7",
    "@editorjs/quote": "^2.7.6",
    "@editorjs/simple-image": "^1.6.0",
    "@editorjs/underline": "^1.2.1",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.16.2",
    "aws-sdk": "^2.1692.0",
    "jszip": "^3.10.1",
    "marked": "^16.3.0",
    "next": "15.5.3",
    "next-intl": "^4.3.9",
    "prisma": "^6.16.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "react-pdf": "^10.1.0",
    "turndown": "^7.2.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/turndown": "^5.0.5",
    "dotenv": "^16.4.7",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "jest": "^30.1.3",
    "jest-environment-node": "^30.1.2",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
